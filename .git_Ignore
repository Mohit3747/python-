import os

# File to store student data
FILE_NAME = "students.txt"

# Function to add a student
def add_student():
    roll = input("Enter Roll Number: ")
    name = input("Enter Name: ")
    branch = input("Enter Branch: ")

    with open(FILE_NAME, "a") as f:
        f.write(f"{roll},{name},{branch}\n")
    print("‚úÖ Student added successfully!\n")

# Function to view all students
def view_students():
    if not os.path.exists(FILE_NAME):
        print("‚ö† No records found!\n")
        return

    with open(FILE_NAME, "r") as f:
        students = f.readlines()

    print("\n--- Student Records ---")
    for student in students:
        roll, name, branch = student.strip().split(",")
        print(f"Roll: {roll} | Name: {name} | Branch: {branch}")
    print()

# Function to search a student by roll number
def search_student():
    roll_search = input("Enter Roll Number to search: ")

    if not os.path.exists(FILE_NAME):
        print("‚ö† No records found!\n")
        return

    found = False
    with open(FILE_NAME, "r") as f:
        for line in f:
            roll, name, branch = line.strip().split(",")
            if roll == roll_search:
                print(f"\n‚úÖ Found: Roll: {roll} | Name: {name} | Branch: {branch}\n")
                found = True
                break

    if not found:
        print("‚ùå Student not found!\n")

# Main Menu
def main():
    while True:
        print("===== Student Management System =====")
        print("1. Add Student")
        print("2. View All Students")
        print("3. Search Student")
        print("4. Exit")

        choice = input("Enter your choice: ")

        if choice == "1":
            add_student()
        elif choice == "2":
            view_students()
        elif choice == "3":
            search_student()
        elif choice == "4":
            print("üëã Exiting... Goodbye!")
            break
        else:
            print("‚ö† Invalid choice! Try again.\n")

# Run the program
if __name__ == "__main__":
    main()